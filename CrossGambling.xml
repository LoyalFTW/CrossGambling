	<Frame name="CrossGambling_Frame1" parent="UIParent" toplevel="false" movable="true" enableMouse="true">

		<Scripts>
			<OnLoad>
				CrossGambling_OnLoad(self);
			</OnLoad>
			<OnEvent>
				CrossGambling_OnEvent(self, event, ...);
			</OnEvent>
			<OnDragStart>
				self:StartMoving();
				self.isMoving = true;
			</OnDragStart>
			<OnDragStop>
				self:StopMovingOrSizing();
